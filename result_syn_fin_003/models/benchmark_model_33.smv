		(chain_pos = 2) & (!b0 & !b1 & b2 & b3 & b4 & !b5) : 3;
		(chain_pos = 3) & (!b0 & b1 & b2 & !b3 & b4 & b5) : 4;
		(chain_pos = 4) & (b0 & !b1 & b2 & !b3 & !b4 & b5) : 5;
		(chain_pos = 5) & (!b0 & !b1 & !b2 & !b3 & b4 & b5) : 6;
		(chain_pos = 6) & (!b0 & !b1 & b2 & b3 & b4 & !b5) : 7;
		(chain_pos = 7) & (!b0 & b1 & b2 & !b3 & b4 & b5) : 8;
		(chain_pos = 8) & (b0 & !b1 & !b2 & !b3 & b4 & b5) : 9;
		(chain_pos = 9) & (b0 & b1 & b2 & !b3 & b4 & b5) : 10;
