		(chain_pos = 14) & (b0 & !b1 & !b2 & b3 & !b4 & b5) : 15;
		(chain_pos = 15) & (!b0 & !b1 & !b2 & b3 & !b4 & b5) : 0;
		TRUE : chain_pos;
	esac;
DEFINE
	p_0 := (chain_pos = 15) & ((b2 | !b2));
	p_1 := (chain_pos = 15) & (!b2 & b4 & (b0 | !b0));
	p_2 := (chain_pos = 11) & (!b0 & (b5 | !b5));
	p_3 := (chain_pos = 14) & (b3 & (b1 & !b1) & !b5);
