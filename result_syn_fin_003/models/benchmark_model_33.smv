	p_9 := (chain_pos = 3) & ((b5 & !b5) & b4);
	p_10 := (chain_pos = 15) & ((b4 | !b4) & (b2 | !b2));
	p_11 := (chain_pos = 15) & (!b3 & !b1 & (b4 | !b4));
	p_12 := (!b5 & b3);
	p_13 := (chain_pos = 10) & (b0 & (b1 | !b1));
	p_14 := (chain_pos = 9) & ((b5 & !b5) & b1);
	p_15 := (chain_pos = 3) & (b4);
	p_16 := (chain_pos = 13) & ((b4 & !b4));
	p_17 := (chain_pos = 1) & ((b4 | !b4) & (b5 | !b5) & (b2 | !b2));
	p_18 := (chain_pos = 6) & (b5 & (b4 & !b4) & (b1 & !b1));
